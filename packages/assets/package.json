    "./css/argos.css": "./dist/css/argos-custom.css",
    "./css/argos.min.css": "./dist/css/argos-custom.min.css",
    "./css/atlas.css": "./dist/css/atlas-custom.css",
    "./css/atlas.min.css": "./dist/css/atlas-custom.min.css",
